$('table#find_songs').show();
$("table#find_songs > tbody").html("");
<% @songs.each do |song| %>
  $('table#find_songs tbody:last').append('<tr><td><%= song.title %></td></tr>');
<% end %>
<% if @songs.none? %>
    $('table#find_songs tbody:last').append('<tr><td>Nothing found for "<%= params[:artist_name] %>"</td></tr>');
<% end %>

$('table#find_songs').focus();
